{
  "name": "ant-design-pro",
  "version": "6.0.0",
  "private": true,
  "description": "An out-of-box UI solution for enterprise applications",
  "repository": "git@github.com:ant-design/ant-design-pro.git",
  "scripts": {
    "prestart:desktop": "node -e \"const fs=require('fs');const path=require('path');const dir=path.join('src','.umi');fs.mkdirSync(dir,{recursive:true});const file=path.join(dir,'appData.json');if(!fs.existsSync(file)){fs.writeFileSync(file,'{}','utf8');}\"",
    "analyze": "cross-env ANALYZE=1 max build",
    "build": "max build",
    "deploy": "npm run build && npm run gh-pages",
    "dev": "npm run start:dev",
    "api:dev": "node -r ts-node/register/transpile-only backend/devApiServer.ts",
    "gh-pages": "gh-pages -d dist",
    "i18n-remove": "pro i18n-remove --locale=zh-CN --write",
    "postinstall": "max setup",
    "jest": "cross-env UMI_SKIP_OPENAPI=1 jest --runInBand",
    "jest:parallel": "cross-env UMI_SKIP_OPENAPI=1 jest",
    "lint": "npm run biome:lint && npm run tsc",
    "lint-staged": "lint-staged",
    "biome:lint": "npx @biomejs/biome lint",
    "openapi": "max openapi",
    "prepare": "husky",
    "preview": "npm run build && max preview --port 8000",
    "record": "cross-env NODE_ENV=development UMI_ENV=test max record --scene=login",
    "serve": "umi-serve",
    "start": "cross-env UMI_ENV=dev max dev",
    "start:dev": "cross-env UMI_ENV=dev MOCK=none max dev",
    "start:desktop": "npm run prestart:desktop && cross-env UMI_ENV=dev PORT=8003 max dev",
    "start:no-mock": "cross-env MOCK=none max dev",
    "start:pre": "cross-env UMI_ENV=pre MOCK=none max dev",
    "start:test": "cross-env UMI_ENV=test MOCK=none max dev",
    "desktop:electron": "wait-on http://localhost:8003 && cross-env ELECTRON_START_URL=http://localhost:8003 electron ./electron/main.js",
    "desktop:dev": "npm run prestart:desktop && concurrently --kill-others-on-fail -n api,web,desktop -c \"green,cyan,magenta\" \"npm run api:dev\" \"npm run start:desktop\" \"npm run desktop:electron\"",
    "desktop:dist": "concurrently -k -n api,desktop -c \"green,magenta\" \"npm run api:dev\" \"npm run build && electron ./electron/main.js\"",
    "desktop:icons": "node scripts/generate-icons.js",
    "desktop:icons:ico": "node scripts/generate-ico.js",
    "desktop:register": "node scripts/generate-ico.js && node scripts/register-eapkg.js",
    "desktop:build": "npm run build && npm run desktop:icons && npx electron-builder --config electron-builder.json",
    "desktop:build:win": "npm run build && npm run desktop:icons && npx electron-builder --config electron-builder.json --win",
    "desktop:build:mac": "npm run build && npm run desktop:icons && npx electron-builder --config electron-builder.json --mac",
    "test": "npm run jest",
    "test:coverage": "npm run jest -- --coverage",
    "test:update": "npm run jest -- -u",
    "tsc": "tsc --noEmit",
    "loadtest:impact": "node -r ts-node/register/transpile-only backend/loadtest/runImpactLoadTest.ts"
  },
  "browserslist": [
    "defaults"
  ],
  "dependencies": {
    "@ant-design/icons": "^6.1.0",
    "@ant-design/pro-components": "^2.8.9",
    "antd": "^6.0.0",
    "antd-style": "^3.7.0",
    "classnames": "^2.5.1",
    "cytoscape": "^3.28.1",
    "dayjs": "^1.11.13",
    "fflate": "^0.8.2",
    "neo4j-driver": "^5.28.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-resizable": "^3.1.3",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@ant-design/pro-cli": "^3.3.0",
    "@biomejs/biome": "^2.1.1",
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.3.0",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.3",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/react-helmet": "^6.1.11",
    "@umijs/lint": "^4.6.13",
    "@umijs/max": "^4.6.13",
    "concurrently": "^9.2.1",
    "cross-env": "^10.0.0",
    "dotenv": "^16.6.1",
    "electron": "^29.4.6",
    "express": "^5.2.0",
    "gh-pages": "^6.1.1",
    "husky": "^9.1.7",
    "jest": "^30.0.4",
    "jest-environment-jsdom": "^30.0.5",
    "lint-staged": "^16.1.2",
    "mockjs": "^1.1.0",
    "sharp": "^0.34.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3",
    "umi-presets-pro": "^2.0.3",
    "umi-serve": "^1.9.11",
    "wait-on": "^8.0.5"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "create-umi": {
    "ignoreScript": [
      "docker*",
      "functions*",
      "site",
      "generateMock"
    ],
    "ignoreDependencies": [
      "netlify*",
      "serverless"
    ],
    "ignore": [
      ".dockerignore",
      ".git",
      ".github",
      ".gitpod.yml",
      "CODE_OF_CONDUCT.md",
      "Dockerfile",
      "Dockerfile.*",
      "lambda",
      "LICENSE",
      "netlify.toml",
      "README.*.md",
      "azure-pipelines.yml",
      "docker",
      "CNAME",
      "create-umi"
    ]
  }
}
